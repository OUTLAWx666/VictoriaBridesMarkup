@require "_mixins"

/* Landing page */
body.lp
   background: #f2f0ea;

/* plugins */
.select2-container
   .select2-choice
      .select2-arrow
         b
            background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAkAAAAFCAMAAACgjTZZAAAAPFBMVEXgawX////gawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawUasSd3AAAAFHRSTlMAAGW7MXuOKRABBJavtQo2vwu0iSHQ3LwAAAAxSURBVHheHcRBFgAQCAXAn5IE4P535ZnFILAQkXBAji1R2jEDrnVUdTy9zLLw2bH3BRmaAQ9KRCKXAAAAAElFTkSuQmCC')
.select2-drop-active
   border-color: #fff

/* default */
body
   background: #f2f0ea
   color: #5b5b5b
   &.pm
      background: #f2f0ea;

/* btn */
.b-btn
   font-size: 16px;
   font-weight: normal;
   border-radius: 3px;
   border-bottom-width: 2px;
   &:active
      border-top-width: 2px;
      border-bottom: none;
   &:hover
      box-shadow: 0 0 5px rgba(255, 255, 0, 0.9);
   &.blue, &.blue:hover
      background-color: #357fbd
      border-color: #3d6894
   &.green, &.green:hover
      background-color: #56b53e
      border-color: #258f0a
   &.red, &.red:hover
      background-color: #e74e4e
      border-color: #bd2020

/* btn_act */
.b-btn_act
   font-size: 12px;
   text-transform: none;
   color: #333333
   &.border
      border-color: #e9e6dc
      background-color: #f2f0ea
   &.chat
      &:before
         background-image: url('data:image/png;base64,');
   &.like
      &:before
         background-image: url('data:image/png;base64,');
   &.addtof
      &:before
         background-image: url('data:image/png;base64,')
   &.sendwink
      &:before
         background-image: url('data:image/png;base64,')
      &.border:before
         left: 6px;
   &.virtualgift
      &:before
         background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA8AAAASBAMAAAB2sJk8AAAAMFBMVEVEeLhPgLxcicFnkcVzmsp/os6Kq9OWs9ehu9usw9+4y+PD0+jP3Orb5ern7ery8Op9yGahAAAAW0lEQVR4XmP4xGD7/zsD1weGAwyM/wsYWD4wPGRanMdewPGB4a/CfMt1BvUfGP7/Cg9YVPf/A4OLi0stEDcwAIEPEDsgGB0dHeuAeANDAETkAoMDhHGAPAbcHAD5KCi/CrdiSwAAAABJRU5ErkJggg==')

/* more_act */
.b-more_act
   .more_link
      &:after
         background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAkAAAAFCAMAAACgjTZZAAAAPFBMVEXgawX////gawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawXgawUasSd3AAAAFHRSTlMAAGW7MXuOKRABBJavtQo2vwu0iSHQ3LwAAAAxSURBVHheHcRBFgAQCAXAn5IE4P535ZnFILAQkXBAji1R2jEDrnVUdTy9zLLw2bH3BRmaAQ9KRCKXAAAAAElFTkSuQmCC')

/* head */
.b-head
   background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAAEAgMAAADUn3btAAAACVBMVEUwLSsyLyw0MS7btXo3AAAAEElEQVR4XmNYwLCUgYshCgAIGQGq74Ft9AAAAABJRU5ErkJggg==');
   border-bottom-color: #fff

/* foot */
.b-foot
   background-color: #2e2e2e;
   .in
      color: #6d6d6d;
   a
      border-bottom: 1px dotted #a4a39f;
      color: #a4a39f;
      &:hover
         text-decoration: none

/* main_menu */
.b-main_menu
   .item_link
      &:hover
         color: #e86f06
         text-shadow 1px 1px 2 e1b0c
   .prof_photo_wrap
      &:after
         height: 5px;
         background-position: 0 0
         background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAkAAAAFCAMAAACgjTZZAAAAOVBMVEX///////////////////////////////////////////////////////////////////////////+0EB4eAAAAEnRSTlMAh0L5AaS9NxUGyOpIDvEP8LepotYaAAAALklEQVR4Xh3Ahw2AMAwAwXd3CnX/YZE4xBRQEzzOou5wGLn2ygFwzHde/Ppp4AMVgwDuIkD8ygAAAABJRU5ErkJggg==')
         transition transform .15s ease-in
   .prof.selected
      .prof_photo_wrap
         &:after
            background-position: 0 0
            transform: rotate(180deg)
   .submenu
      top: 40px
      background-color: #34312e;
      border: none
      a
         color: #e99348;
         &:hover
            background-color: #594e44

/* b-pagination */
.b-pagination
   .item
      height: 28px
      width: @height
      margin-right: 25px;
      line-height: @height
      border: none
      background: transparent
      color: #e86f06
      &:hover
         background-color: transparent;
         box-shadow: inset 0 0 20px rgba(255, 255, 255, 1);
      &.disable
         color: #999999
   .prev, .next
      width: 80px
      background-color: #fff
      border-bottom: 2px solid #d9d6ce
      border-radius: 3px;
      text-transform: lowercase
      color: #5b5b5b
      &:hover
         background-color: #fff
         box-shadow: none;
      &.disable
         background-color: transparent

/* sb */
.b-sb
   border: none;
   border-bottom: 2px solid #d0cdc5;
   .title
      font-size: 16px
      background: transparent
      border-bottom: 1px solid #e7e6e2
      color: #5b5b5b
   .item_name
      font-weight: normal
   .imp
      border: 1px solid #e7e6e2
      background: transparent
      font-weight: bold
      color: #e74e4e
      text-shadow none
      padding-top: 2px
      padding-bottom: @padding-top
.b-sb_up3
   border: 1px solid #d0cdc5
.b-sb_sp
   .name
      color: #333333
   .loc
      color: #999;
   .online
      padding-top: 0
      padding-bottom: @padding-top
      background-color: #56b53e
      text-transform: lowercase

/* b-filters */
.b-filters
   background-color: #fff;
   border: none
   border-bottom: 1px solid #e9e6dc
   border-radius: 3px
   .filter
      padding-top: 2px
      padding-bottom: @padding-top
      font-size: 16px
      font-weight: normal
      &.selected
         background-color: #e86f06
         color: #fff
   .match
      color: #787878
      #found
         color: #e86f06

/* search */
.b-search
   .select2-container
      width: 60px;
   .select2-container
      .select2-choice
         border: 1px solid #ffffff;
         box-shadow: inset 2px 2px 3px rgba(0, 0, 0, .1), 0 1px 1px #e9e6dc;
         background-color: #fff;

/* prfs */
.b-prfs
   .item
      border-radius: 3px;
      border-right: 3px
      border-bottom: 2px solid #d0cdc5

/* user_status  */
.b-user_status
   .property
      color: #5b5b5b;
      background-color: rgba(255, 255, 255, 0.7);
   .photos
      &:before
         background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA8AAAALBAMAAABSacpvAAAAJ1BMVEX////obwbobwbobwbobwbobwbobwbobwbobwbobwbobwbobwbobwbzvDazAAAADHRSTlMAEDBAUHCAj5+/3+9ueiPCAAAAS0lEQVR4XmOwPHMsLS3zuALDGRA4bLoBxNgJZBwAMk4ISJ45A2IsYGDcA2ZMYGCYAxVhWANmnGDgOANmnFm5BswAgzMbGNzSwMAAAGHvNT6vaX4yAAAAAElFTkSuQmCC')
   .desc
      &:before
         background-image: url('data:image/png;base64,')
   .online
      padding-left: 10px
      padding-right: @padding-left
      background-color: #56b53e
      font-size: 12px
      text-transform lowercase
/* content */
.b-content
   &.pp
      .users_wrap
         background-color: #fff
      .user_ttl
         padding-top: 24px;
         padding-bottom: @padding-top
         font-size: 16px
         text-transform: uppercase
      .waiting
         border-top: 1px solid #e7e6e2;
         border-bottom: @border-top
         background-color: #fff
         color: #5b5b5b
         font-size: 16px;
         line-height: 18px;
         padding-top: 6px;
         padding-bottom: 6px;
      .list_users
         .name
            color: #333333
         .online
            background-color: #56b53e;
            text-transform: lowercase
         .loc
            color: #9ca3a7;
      .ch
         font-size: 16px
         color: #787878
      .bs
         margin-top: 16px;
         text-decoration: none
         color: #5b5b5b
         border-bottom: 1px dotted #5b5b5b
      .ad
         font-size: 16px
         background-color: #ffffff
         border-bottom: 1px solid #e9e6dc
         margin-bottom: 8px;
         &:before
            border-bottom: 1px solid #e9e6dc
            background-color: #ff6600;
      .pf
         color: #333333
      .imp
         color: #357fbd
      .prices
         background-color: #357fbd
         color: #c4e4ff
      .price
         color: #ffffff
      .cur
         color: #c4e4ff
      .tariff_bv
         .prices
            background-color: #3d6894;
            border-radius: 0
         .bv
            top: -28px
            background-color: #357fbd;
            line-height: 28px;
            text-transform: none;
      .plus
         color: #d4d2cb
      .tariff_info
         background-color: #ffffff
         border: none
         border-bottom: 2px solid #d0cdc5
      .avoid
         color: #999999
      .not_ready
         border: none
         border-top: 1px solid #fff
         border-bottom: 1px solid #e9e6dc
         background-color: #ffffff
         .credits_wrap
            border-bottom: 1px solid #e9e6dc;
            background-color: #e74e4e
         .ttl
            font-size: 16px;
         .txt
            font-size: 16px;
         .cur
            color: #ffd1d1
      .info_cell
         border-top: none;
      .ttl_sub, .ttl_cr
         line-height: 18px;
      .ttl_cr
         font-weight: bold
      .info_table
         li:before
            width: 15px
            background-image: url('data:image/png;base64,')
      .ec
         color: #999999
         .link
            color: #999999

/* like */
.b-like

   .title
      padding: 25px 0 16px
      color: #413b36
   .btns1
      overflow: visible;
      clear()
   .btn_skip
      background-color: #dcd8cc
      border-color: #bbb7a9
      color: #a09b8b
      &:before
         width: 8px
         height: 13px
         right: 55px;
         top 50%
         margin-top: -6.5px
         background-image: url('data:image/png;base64,')
   .property
      height: 30px
   .photos
      font-size: 12px
      line-height: 30px
      &:before
         top: 2px
   .online
      font-weight: normal
      line-height: 30px
      background-color: #56b53e
   .brief
      padding-top: 15px
      padding-bottom: @padding-top
   .loc
      color: #999999
   .name_age
      color: #5b5b5b
   .name
      color: #333333

/* like_gallery_block */
.b-like_gallery_block
   border-top: 1px solid #e7e6e2
   border-bottom: 2px solid #d0cdc5
   background-color: #fff;
   .ttl_gallery
      padding: 15px 0 20px 25px
   .gallery
      .item
         &.selected
            &:after
               border-color: #e86f06
      